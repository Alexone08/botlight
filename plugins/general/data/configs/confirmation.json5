{
    config: {
        // If set to false certain commands and buttons will execute without confirmation (Boolean)
        enabled: true,
        // Time before the confirmation process will be automatically canceled (Integer)
        timeout_in_seconds: 30,

        // Commands that require confirmation (Array)
        commands_require_confirmation: [
            "restart",
            "example_2",
            "example_3",
        ],

        // Buttons, Menus, Models that that require confirmation - you can find a list of all names in permission.yml (Array)
        interaction_require_confirmation: [
            "none",
            "example_4",
        ]
    },




    // ########################################################
    // #                                                      #
    // #  Language Configuration (not required to configure)  #
    // #                                                      #
    // ########################################################

    lang: {}
}